{
  "name": "mindee/mindee",
  "description": "Mindee Client Library for PHP",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "symfony/console": ">=5.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.38",
    "squizlabs/php_codesniffer": "^3.7",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Mindee\\": "src"
    }
  },
  "bin": [
    "mindee",
    "bin/cli.php"
  ]
}
